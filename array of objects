#include<iostream>
using namespace std;
class employee
{
	int id;
	char name[30];
	public:
		void getdata();
		void putdata();
	};
	void employee::getdata()
	{
		cout<<"enter id:";
		cin>>name;
	}
	void employee::putdata()
	{
		cout<<id<<"";
		cout<<name<<"";	
		cout<<endl;
	}
	int main()
	{
		employee emp[30];
		int n,i;
		cout<<"enter number of employees-";
		cin>>n;
		for(i=0;i<n;i++)
		emp[i].getdata();
		cout<<"employee data-"<<endl;
		for(i=0;i<n;i++)
		emp[i].putdata();
	}
	
